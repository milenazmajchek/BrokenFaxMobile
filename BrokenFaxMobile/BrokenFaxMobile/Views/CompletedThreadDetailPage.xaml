<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BrokenFaxMobile.Views.CompletedThreadDetailPage"
             Title="{Binding Term}">

    <StackLayout Spacing="20" Padding="15">
        <Label Text="Creator:" FontSize="Medium" />
        <Label Text="{Binding CreatorName}" FontSize="Small"/>
        <Label Text="Group Name:" FontSize="Medium" />
        <Label Text="{Binding GroupName}" FontSize="Small"/>
    </StackLayout>
    
</ContentPage>